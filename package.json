{
  "name": "flow-boilerplate",
  "version": "0.1.0",
  "description": "test flow boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "npm install && npm run install:types",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "flow": "flow",
    "typecheck": "flow check",
    "preinstall:types": "rm -rf flow/flow-typed",
    "install:types": "flow-typed install && mv flow-typed flow"
  },
  "author": "Saad Quadri",
  "license": "MIT",
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.0",
    "flow-bin": "^0.47.0",
    "html-webpack-plugin": "^2.28.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
